pipeline {
    agent any

    stages {
        stage('Greet') {
            steps {
                echo 'Hello from Jenkins Pipeline!'
            }
        }

        stage('Info') {
            steps {
                echo 'Running the Info stage...'
            }
        }

        stage('Done') {
            steps {
                echo 'Pipeline completed successfully.'
            }
        }
    }
}
